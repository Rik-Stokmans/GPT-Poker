@model SectionViewModel

<head>
    <meta charset="UTF-8">
    <title>GPT-Poker</title>
    <link rel="stylesheet" href="~/css/Components.css">
    <link rel="stylesheet" href="~/css/Fonts.css">
</head>
<body>

<div id="content-left">
    <div id="content-left-content" style="padding-top: 50px !important;">
        
        @foreach (var unit in Model.Progress.SectionProgress.Keys)
        {
            var lessonProgressDict = Model.Progress.SectionProgress[unit];
            
            @await Html.PartialAsync("UnitPartial", new UnitProgressViewModel(unit, lessonProgressDict))
            
        }
        
    </div>
</div>

<div id="content-right">
    <div id="content-right-navbar">

    </div>

    <div id="content-right-content">

    </div>
</div>

</body>